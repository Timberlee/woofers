<table>
  <tr>Users:</tr>
  <tr>
    <th>id</th>
    <th>First</th>
    <th>Last</th>
    <th>Email</th>
    <th>Birthday</th>
    <th>Joined</th>
  </tr>
  <tr>
    <% @users.each do |user| %>
        <td><%=user.id%></td>
        <td><%=user.fname%></td>
        <td><%=user.lname%></td>
        <td><%=user.email%></td>
        <td><%=user.birthday%></td>
        <td><%=user.created_at%></td>
  </tr>

    <% end %>

</table>
